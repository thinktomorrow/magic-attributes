{
  "name": "thinktomorrow/magic-attributes",
  "description": "Magic assistance for retrieving nested properties",
  "keywords": [
    "think tomorrow",
    "magic-attributes"
  ],
  "homepage": "https://github.com/thinktomorrow/magic-attributes",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Cavens",
      "email": "ben@thinktomorrow.be",
      "homepage": "https://thinktomorrow.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "illuminate/database": "^5.6",
    "illuminate/support": "^5.6",
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Thinktomorrow\\MagicAttributes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thinktomorrow\\MagicAttributes\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  }
}
